{
    "name": "bfg/comcode",
    "description": "",
    "type": "bfg-app",
    "keywords": [
        "extension",
        "comcode"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "bfg"
        }
    ],
    "prefer-stable": true,
    "require": {
        "php": ">=8.0.0",
        "nikic/php-parser": "^v4.14.0",
        "friendsofphp/php-cs-fixer": "^v3.13.2",
        "kubawerlos/php-cs-fixer-custom-fixers": "^3.11",
        "pedrotroller/php-cs-custom-fixer": "^2.28",
        "symplify/easy-coding-standard": "^11.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10",
        "symfony/var-dumper": "^v6.0.9"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "autoload": {
        "psr-4": {
            "Bfg\\Comcode\\": "src/",
            "Bfg\\Comcode\\Tests\\": "tests/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Bfg\\Comcode\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/comcode"
    ]
}
